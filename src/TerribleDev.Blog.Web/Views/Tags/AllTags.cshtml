@model Dictionary<string, List<IPost>>
@{
    ViewData["Title"] = "all-tags";
    ViewData["RenderNav"] = true;
}
<h2>All Tags</h2>
@foreach(var tag in Model.Keys)
{
        <a href="#" class="btn">@tag</a>
}
@section Head {
    <style>
        .btn {
            display: inline-block;
            margin-bottom: 4px;
            margin-right: 4px;
            font-size: .9rem;
        }
    </style>
}