@model IPost
@{
    ViewData["Title"] = "Post";
}

<article class="main-content-wrap" itemprop="blogPost">
    <h1 itemprop="headline">@Model.Title</h1>
    <time itemprop="datePublished" content="@Model.PublishDate.ToString()">@Model.PublishDate.ToString("D")</time>
</time>

    @Model.Content
</article>